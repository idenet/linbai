.header {
	display: flex;
	height: 100rpx;
	flex-direction: row;
	align-items: center;
	justify-content: space-between;
	border-top: 1rpx solid #f3f3f3;
	box-shadow: 0px 2px 12px 0px rgba(201, 201, 201, 0.1);
}
.header-left {
	display: flex;
	flex-direction: row;
	align-items: center;
	margin-left: 30rpx;
	text-align: left;
}

.no {
	display: flex;
	height: 58rpx;
	font-size: 32rpx;
	align-items: flex-end;
}

.index {
	font-size: 58rpx;
}

.line {
	height: 44rpx;
	border-left: 2rpx solid #010101;
	margin-left: 14rpx;
	margin-right: 14rpx;
}
.date {
	display: flex;
	flex-direction: column;
	margin-bottom: 10rpx;
}
.month {
	height: 24rpx;
	font-size: 24rpx;
	margin-bottom: 4rpx;
}
.year {
	height: 20rpx;
	font-size: 20rpx;
}
.header-right {
	position: relative;
	margin-right: 30rpx;
	width: 140rpx;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
}

.like-image {
	width: 32rpx;
	height: 28rpx;
}

.like-num {
	position: absolute;
	left: 33rpx;
	top: 24rpx;
	font-size: 18rpx;
	color: #bbbbbb;
}

.header-right-url {
  position: relative;
	width: 100rpx;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
}

.like-num-url {
	position: absolute;
	left: 33rpx;
	top: -12rpx;
	font-size: 18rpx;
	color: #bbbbbb;
}
